{
  "name": "pos-leaf",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win nsis"
  },
  "devDependencies": {
    "electron": "^24.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.grintsys.leafpos",
    "asar": true,
    "files": [
      "**/*"
    ],
    "directories": {
      "buildResources": ".",
      "output": "dist"
    },
    "win": {
      "icon": "leaf.ico",
      "target": [
        "nsis"
      ]
    }
  }
}
